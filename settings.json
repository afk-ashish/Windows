{
  "chat.commandCenter.enabled": false,
  "window.commandCenter": false,
  "workbench.statusBar.visible": false,

  "autoHide.sideBarDelay": 45,
  "autoHide.panelDelay": 30,
  "workbench.colorCustomizations": {
    "terminal.background": "#00000000"
  },
  "vscode_vibrancy.opacity": 0.1,
  "vscode_vibrancy.preferedDarkTheme": "GitHub Dark Default",
  "vscode_vibrancy.theme": "GitHub Dark Default",

  "editor.fontSize": 22,
  "editor.fontFamily": "'Cascadia Code', Consolas, 'Courier New', monospace",

  "files.autoSave": "afterDelay",

  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.renderLineHighlight": "none",

  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "expand",
"editor.cursorStyle": "line",
"window.titleBarStyle": "custom",

  "workbench.iconTheme": "material-icon-theme",
  "explorer.openEditors.visible": 1,
  "explorer.compactFolders": false,
  "explorer.decorations.badges": false,
  "explorer.decorations.colors": false,
  "workbench.tree.indent": 10,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.tree.expandMode": "singleClick",
  "workbench.activityBar.location": "top",
  "vscode_vibrancy.imports": [

    "/path/to/file"
  ],
  "vscode_vibrancy.type": "acrylic",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.stickyScroll.enabled": false,
  "code-runner.saveFileBeforeRun": true,
"code-runner.runInTerminal": true,
"workbench.secondarySideBar.defaultVisibility": "hidden",
"editor.mouseWheelZoom": true,
"workbench.secondarySideBar.showLabels": false,
"livePreview.customExternalBrowser": "Chrome",
"livePreview.openPreviewTarget": "External Browser",
"vscode_vibrancy.disableFramelessWindow": true,
"workbench.colorTheme": "GitHub Dark Default",
"terminal.integrated.gpuAcceleration": "off",
"window.systemColorTheme": "dark",
"window.autoDetectColorScheme": false,
"window.controlsStyle": "custom"
}
